: 1626151240:0;get_nmcli_table() {\
    # Use tab ('\t') as delimiter for SSID\
    # Pass callback filter as argument to allow on-the-fly filtering\
    # Hackish, but it seems to work\
    $* <(\
        nmcli -t -c yes \\
            -f "bssid,ssid,signal,bars,freq" \\
            dev wifi list 2>/dev/null |\
            sed 's/^\([^:]*\):/\1\t:/1' |\
            column -t -s:\
    )\
}
