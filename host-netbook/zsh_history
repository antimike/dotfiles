/[^\s]|/ {\
    printf "Matched line with internal column separator\n"\
    printf "Original line: %s\n", $0\
    tabular = $0\
    while (getline && $0 !~ /|\s*$/) {\
        tabular += " " + $0\
    }\
    tabular += $0\
    split(tabular, cols, /\s*|\s*/)\
    printf "num_cols: %s\n", length($cols)\
    "bc <<< '80/" length($cols) "'" | getline colwidth\
    printf "colwidth: %s\n", $colwidth\
    concat_cmd = "paste "\
    printf "Entering loop...\n"\
    for (colnum in cols) {\
        cmd = "fold -s -w " $colwidth " " $cols[colnum]\
        $cmd | getline cols[$colnum]\
        concat_cmd += "<(echo '" $cols[$colnum] "') "\
        printf "\tcolnum: %s\n\tconcat_cmd: %s\n", $colnum, $concat_cmd\
    }\
    $concat_cmd | getline\
    print\
    next\
}\
print\
}\
AWK
