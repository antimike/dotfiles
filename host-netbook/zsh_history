: 1626305821:0;read -r -d '' awk_script <<-"AWK"\
{\
    if ($0 ~ /\|/) {\
        tabular = $0\
        while ($0 !~ /\|\s*$/) {\
            getline && tabular = tabular " " $0\
        }\
        # Set RS here---we'll need it to be "|" for the next getline\
        RS = "|"\
        split(tabular, cols, /\|/)\
