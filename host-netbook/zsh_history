        printf "Original line: %s\n", $0\
        tabular = $0\
        while (getline && $0 !~ /\|\s*$/) {\
            tabular = tabular " " + $0\
        }\
        # tabular = tabular $0\
        printf "tabular: %s\n", $0\
        split(tabular, cols, /\|/)\
        num_cols = length(cols)\
        printf "num_cols: %d\n", num_cols\
        # "bc <<< '80/" length(cols) "'" | getline colwidth\
        # printf "colwidth: %s\n", colwidth\
        concat_cmd = "paste"\
        for (colnum in cols) {\
            printf "\tcolnum: %s\n", colnum\
            printf "\tcol: %s\n", cols[colnum]\
            cmd = "fold -s -w $(( 80/" num_cols " )) <<< '" cols[colnum] "'"\
            printf "\tcmd: %s\n", cmd\
            "bash -c \"" cmd "\"" | getline cols[colnum]\
            printf "\tFolded column: %s\n", cols[colnum]\
            concat_cmd = concat_cmd " <(echo '" cols[colnum] "')"\
            printf "\tconcat_cmd: %s\n", concat_cmd\
        }\
        printf "Exited loop.\n"\
        printf "concat_cmd: %s", concat_cmd\
        "bash -c \"" concat_cmd "\"" | getline\
        print\
        next\
    }\
    print\
}\
AWK
