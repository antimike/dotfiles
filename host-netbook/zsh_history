            printf "\tFolded column: %s\n", cols[colnum]\
            concat_cmd += "<(echo '" cols[colnum] "') "\
            printf "\tconcat_cmd: %s\n", concat_cmd\
        }\
        printf "Exited loop.\n"\
        printf "concat_cmd: %s", concat_cmd\
        concat_cmd | getline\
        print\
        next\
    }\
    print\
}\
