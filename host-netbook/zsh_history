{\
    printf "================ PASS " ++i " ===================\n"\
    if ($0 ~ /[^\s].*|/) {\
        printf "Matched line with internal column separator\n"\
        printf "Original line: %s\n", $0\
        tabular = $0\
        while (getline && $0 !~ /|\s*$/) {\
            tabular += " " + $0\
        }\
        tabular += $0\
        printf "tabular: %s\n", $0\
        split(tabular, cols, /\s*|\s*/)\
        num_cols = length(cols)\
        printf "num_cols: %d\n", num_cols\
        # "bc <<< '80/" length(cols) "'" | getline colwidth\
        # printf "colwidth: %s\n", colwidth\
        concat_cmd = "paste "\
        printf "Entering loop...\n"\
